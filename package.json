{
  "name": "iliketomatoes",
  "version": "0.1.0",
  "description": "Giancarlo Soveriniâ€™s personal website",
  "author": "Giancarlo Soverini <giancarlosoverini@gmail.com>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/iliketomatoes/iliketomatoes"
  },
  "keywords": [
    "iliketomatoes"
  ],
  "license": "AGPL-3.0",
  "scripts": {
    "clean-dist": "rimraf dist",
    "clean-coverage": "rimraf scr/tests-reports",
    "start": "parcel src/index.html",
    "lint": "eslint src/scripts/*",
    "build": "parcel build src/index.html --public-url ./ --out-dir dist",
